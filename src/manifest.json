{
  "manifest_version": 3,
  "name": "Placid",
  "description": "Configures Chrome in a way that hardens some boundaries, reducing distraction.",
  "version": "0.1",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "chrome_url_overrides": {
    "newtab": "ntp.html"
  },

  "background": {
    "service_worker": "sw.js"
  },
  "permissions": [ "contentSettings", "privacy" ]
}
